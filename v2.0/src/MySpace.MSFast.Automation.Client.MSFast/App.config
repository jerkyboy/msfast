<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="appConfiguration" type="EYF.Core.Configuration.EYFConfigurationSectionHandler, EYF.Core" />
  </configSections>

  <appConfiguration>
    <add key="CheckInterval" value="10000" />
    <add key="MSTest" value="C:\Program Files\Microsoft Visual Studio 9.0\Common7\IDE\MSTest.exe" />
    <add key="TestTempFolder" value="C:\Temp" />
    <add key="ResultsFilePattern" value="C:\Temp\Results\WebtestResults_{0}.trx" />
    <add key="MSTestTimeout" value="30000" />
    <add key="ClientKey" value="clientkey" />
    <add key="ClientID" value="clientid" />
    <add key="BaseDomain" value="modules.corp.myspace.com" />
  </appConfiguration>
  
  <log4net debug="false">
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="./log/task.log" />
      <param name="AppendToFile" value="true" />
      <maxSizeRollBackups value="0" />
      <maximumFileSize value="1MB" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] &lt;%X{auth}> - %m%n" />
      </layout>
    </appender>
    <logger name="DEFAULT">
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
    </logger>
  </log4net>
</configuration>